local function on_specific_character_dialog_list(character_id, dialogs)
	
	if not dialogs then
		return
	end
	
	if character_id:find("sim_default_") then
		if not dialogs.has("dl_xcvb_modik_dialog") then
			dialogs:add("dl_xcvb_modik_dialog", 3)
		end
	end
	
end

function on_xml_read()
	RegisterScriptCallback("on_specific_character_dialog_list", on_specific_character_dialog_list)
end